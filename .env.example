# Firebase Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

REACT_APP_FIREBASE_API_KEY=your_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id
REACT_APP_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Cloudflare Pages Publishing Configuration
REACT_APP_CLOUDFLARE_ACCOUNT_ID=your_account_id_here
REACT_APP_CLOUDFLARE_API_TOKEN=your_api_token_here
REACT_APP_CLOUDFLARE_PROJECT_NAME=your_pages_project_name
